{
  "name": "sosol-client",
  "version": "0.1.0",
  "private": false,
  "author": "@harkl",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "program:cli": "ts-node src/package/index.ts",
    "program:build": "cd program && cargo build-bpf",
    "program:deploy": "npm run program:build && solana program deploy ./program/target/deploy/sosol_program.so",
    "program:test": "cd program && cargo test-bpf",
    "localnet:up": "solana-test-validator --bpf-program BpfProgram1111111111111111111111111111111111 program/target/deploy/sosol_program.so",
    "localnet:reset": "solana-test-validator --reset",
    "localnet:logs": "solana logs -u localhost",
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "deploy": "gh-pages -d build",
    "deploy:ar": "arweave deploy-dir build --key-file ",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\""
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@apollo/client": "^3.3.20",
    "@craco/craco": "^6.2.0",
    "@ledgerhq/hw-transport-webusb": "^6.0.2",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@project-serum/serum": "^0.13.41",
    "@project-serum/sol-wallet-adapter": "^0.2.5",
    "@solana/spl-token": "^0.1.6",
    "@solana/spl-token-registry": "^0.2.168",
    "@solana/spl-token-swap": "0.1.0",
    "@solana/wallet-adapter-base": "^0.5.2",
    "@solana/wallet-adapter-material-ui": "^0.9.3",
    "@solana/wallet-adapter-react": "^0.9.2",
    "@solana/wallet-adapter-react-ui": "^0.1.1",
    "@solana/wallet-adapter-wallets": "^0.7.6",
    "@solana/web3.js": "^1.20.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/react-router-dom": "^5.1.7",
    "antd": "^4.16.6",
    "axios": "0.21.1",
    "bigint-buffer": "^1.1.5",
    "bignumber.js": "^9.0.1",
    "bs58": "^4.0.1",
    "buffer-layout": "^1.2.1",
    "craco-less": "^1.18.0",
    "emoji-mart": "^3.0.1",
    "eventemitter3": "^4.0.7",
    "graphql": "^15.5.1",
    "husky": "^7.0.0",
    "jazzicon": "^1.5.0",
    "linkifyjs": "^3.0.0-beta.3",
    "moment": "2.29.1",
    "mz": "^2.7.0",
    "notistack": "^1.0.9",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-apollo": "3.1.5",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "^4.0.3",
    "react-textarea-autosize": "8.3.3",
    "react-toastify": "^7.0.4",
    "reactjs-popup": "^2.0.5",
    "styled-components": "5.3.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^26.0.23",
    "@types/ledgerhq__hw-transport": "^4.21.3",
