    "@types/react-dom": "^17.0.8",
    "@types/styled-components": "5.1.11",
    "arweave-deploy": "^1.9.1",
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sosol-gmi/sosol-client"
  },
  "workspaces": [
    "packages/sosol-web3"
  ]
